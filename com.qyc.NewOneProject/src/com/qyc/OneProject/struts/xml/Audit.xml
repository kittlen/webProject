<struts>
	<package name="Audit" extends="struts-default" namespace="/Audit">
		<action name="update" class="com.qyc.OneProject.action.AuditAction"
			method="update">
			<result name="success">/WEB-INF/content/Admin/Audit/Audit.jsp</result>
		</action>

		<!-- 管理员审核 -->
		<action name="AuditInfo" class="com.qyc.OneProject.action.AuditAction"
			method="AuditInfo">
			<result name="success">/WEB-INF/content/Admin/Audit/Audit.jsp</result>
		</action>

		<action name="Audit">
			<result name="success">/WEB-INF/content/Admin/Audit/Audit.jsp</result>
		</action>
	</package>
</struts>

