<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.qyc.OneProject.Model">
	<class name="VCourseSchedule" table="V_CourseSchedule">
		<id name="ScheduleID" type="integer" column="ScheduleID"></id>
		<property name="Course" column="Course" type="string"></property>
		<property name="Name" column="Name" type="string"></property>
		<property name="ClassRoomCategory" column="ClassRoomCategory" type="string"></property>
		<property name="ClassRoomName" column="ClassRoomName" type="string"></property>
		<property name="ProfessionName" column="ProfessionName" type="string"></property>
		<property name="Classfestival" column="Classfestival" type="string"></property>
		<property name="ClassTime" column="ClassTime" type="string"></property>
		<property name="StudyTime" column="StudyTime" type="integer"></property>
		<property name="Crediy" column="Crediy" type="double"></property>
		<property name="CurriculumTime" column="CurriculumTime" type="string"></property>
		<property name="Curricula" column="Curricula" type="string"></property>
		<property name="TeacherID" column="TeacherID" type="string"></property>
		<property name="TeachTaskID" column="TeachTaskID" type="integer"></property>
		<property name="ClassRoomID" column="ClassRoomID" type="string"></property>
		<property name="CourseID" column="CourseID" type="string"></property>
		<property name="ProfessionID" column="ProfessionID" type="string"></property>
		<property name="Remark" column="Remark" type="text"></property>

	</class>
</hibernate-mapping>
